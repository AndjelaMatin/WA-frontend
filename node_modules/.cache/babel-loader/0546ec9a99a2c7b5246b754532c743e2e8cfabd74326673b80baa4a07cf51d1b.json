{"ast":null,"code":"import Card from \"../components/CardComponent.vue\";\nimport api from '@/services/api';\nexport default {\n  data() {\n    return {\n      searchQuery: '',\n      // Unos korisnika\n      searchResults: [],\n      // Rezultati pretrage\n      allRecipes: [] // Svi recepti iz baze\n    };\n  },\n  methods: {\n    // Dohvaćanje svih recepata iz baze\n    async fetchAllRecipes() {\n      try {\n        const response = await api.get('/api/recepti');\n        this.allRecipes = response.data;\n      } catch (error) {\n        console.error('Greška pri dohvaćanju recepata:', error);\n      }\n    },\n    // Funkcija za pretragu recepata\n    async onSearch() {\n      if (this.searchQuery.trim() === '') {\n        this.searchResults = [];\n        return;\n      }\n      try {\n        const response = await api.get('/api/recepti/pretraga', {\n          params: {\n            naziv: this.searchQuery\n          }\n        });\n        this.searchResults = response.data;\n      } catch (error) {\n        console.error('Greška pri pretrazi recepata:', error);\n      }\n    }\n  },\n  mounted() {\n    // Dohvati sve recepte kada se komponenta montira\n    this.fetchAllRecipes();\n  }\n};","map":{"version":3,"names":["Card","api","data","searchQuery","searchResults","allRecipes","methods","fetchAllRecipes","response","get","error","console","onSearch","trim","params","naziv","mounted"],"sources":["C:\\Users\\MATIN\\Desktop\\web aplikacije\\WA-frontend\\src\\views\\PocetnaStranica.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-3\">\r\n    <h1>Dobrodošli na našu slatku platformu!</h1>\r\n    <div class=\"search-container\">\r\n      <input\r\n        v-model=\"searchQuery\"\r\n        type=\"text\"\r\n        placeholder=\"Pretraži recepte...\"\r\n      />\r\n      <button @click=\"onSearch\">Pretraži</button>\r\n    </div>\r\n\r\n    <!-- Rezultati pretrage -->\r\n    <div v-if=\"searchResults.length\" class=\"search-results\">\r\n      <h2>Rezultati pretrage:</h2>\r\n      <ul>\r\n        <li v-for=\"recept in searchResults\" :key=\"recept._id\">\r\n          <img :src=\"recept.slika\" alt=\"Slika recepta\" />\r\n          <p>{{ recept.naziv }}</p>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n\r\n    <!-- Poruka ako nema rezultata -->\r\n    <div v-else-if=\"searchQuery.trim() !== '' && !searchResults.length\">\r\n      <p>Nema rezultata pretrage za \"{{ searchQuery }}\".</p>\r\n    </div>\r\n    <h1 class=\"mb-4\">Najbolji recepti</h1>\r\n    <div>\r\n      <Card\r\n        v-for=\"card in sortedCards\"\r\n        :key=\"card.id\"\r\n        :card=\"card\"\r\n        class=\"card-item\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Card from \"../components/CardComponent.vue\";import api from '@/services/api';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      searchQuery: '', // Unos korisnika\r\n      searchResults: [], // Rezultati pretrage\r\n      allRecipes: [], // Svi recepti iz baze\r\n    };\r\n  },\r\n  methods: {\r\n    // Dohvaćanje svih recepata iz baze\r\n    async fetchAllRecipes() {\r\n      try {\r\n        const response = await api.get('/api/recepti');\r\n        this.allRecipes = response.data;\r\n      } catch (error) {\r\n        console.error('Greška pri dohvaćanju recepata:', error);\r\n      }\r\n    },\r\n\r\n    // Funkcija za pretragu recepata\r\n    async onSearch() {\r\n      if (this.searchQuery.trim() === '') {\r\n        this.searchResults = [];\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await api.get('/api/recepti/pretraga', {\r\n          params: { naziv: this.searchQuery },\r\n        });\r\n        this.searchResults = response.data;\r\n      } catch (error) {\r\n        console.error('Greška pri pretrazi recepata:', error);\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    // Dohvati sve recepte kada se komponenta montira\r\n    this.fetchAllRecipes();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card-item {\r\n  width: 100%; /* Osigurava da kartica zauzima punu širinu unutar svoje kolone */\r\n}\r\n</style>"],"mappings":"AAwCA,OAAOA,IAAG,MAAO,iCAAiC;AAAC,OAAOC,GAAE,MAAO,gBAAgB;AAEnF,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MAAE;MACjBC,aAAa,EAAE,EAAE;MAAE;MACnBC,UAAU,EAAE,EAAE,CAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP;IACA,MAAMC,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,GAAG,CAACQ,GAAG,CAAC,cAAc,CAAC;QAC9C,IAAI,CAACJ,UAAS,GAAIG,QAAQ,CAACN,IAAI;MACjC,EAAE,OAAOQ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MACzD;IACF,CAAC;IAED;IACA,MAAME,QAAQA,CAAA,EAAG;MACf,IAAI,IAAI,CAACT,WAAW,CAACU,IAAI,CAAC,MAAM,EAAE,EAAE;QAClC,IAAI,CAACT,aAAY,GAAI,EAAE;QACvB;MACF;MAEA,IAAI;QACF,MAAMI,QAAO,GAAI,MAAMP,GAAG,CAACQ,GAAG,CAAC,uBAAuB,EAAE;UACtDK,MAAM,EAAE;YAAEC,KAAK,EAAE,IAAI,CAACZ;UAAY;QACpC,CAAC,CAAC;QACF,IAAI,CAACC,aAAY,GAAII,QAAQ,CAACN,IAAI;MACpC,EAAE,OAAOQ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;MACvD;IACF;EACF,CAAC;EACDM,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACT,eAAe,CAAC,CAAC;EACxB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}