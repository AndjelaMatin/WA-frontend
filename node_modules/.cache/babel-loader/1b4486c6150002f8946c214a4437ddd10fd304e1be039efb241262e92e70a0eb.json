{"ast":null,"code":"import Card from \"../components/CardComponent.vue\";\nimport api from \"@/services/api\";\nexport default {\n  components: {\n    Card\n  },\n  data() {\n    return {\n      cards: [],\n      // Svi recepti iz baze\n      searchResults: [],\n      // Rezultati pretrage\n      searchQuery: \"\",\n      // Trenutni pojam pretrage\n      isSearchActive: false // Stanje pretrage\n    };\n  },\n  async created() {\n    // Dohvati sve recepte prilikom učitavanja stranice\n    await this.fetchAllRecepti();\n\n    // Postavi listener za događaj pretrage\n    window.addEventListener(\"search-results\", this.updateSearchResults);\n  },\n  beforeUnmount() {\n    // Ukloni listener za pretragu prilikom uništenja komponente\n    window.removeEventListener(\"search-results\", this.updateSearchResults);\n  },\n  methods: {\n    async fetchAllRecepti() {\n      try {\n        const response = await api.get(\"/recepti\");\n        this.cards = response.data;\n      } catch (error) {\n        console.error(\"Greška pri dohvaćanju recepata:\", error);\n      }\n    },\n    updateSearchResults(event) {\n      // Ažuriraj rezultate pretrage\n      const {\n        data,\n        searchQuery\n      } = event.detail;\n      this.searchResults = data;\n      this.searchQuery = searchQuery;\n      this.isSearchActive = true; // Aktiviraj stanje pretrage\n    },\n    resetSearch() {\n      // Resetiraj pretragu i vrati prikaz svih recepata\n      this.isSearchActive = false;\n      this.searchResults = [];\n      this.searchQuery = \"\";\n    }\n  }\n};","map":{"version":3,"names":["Card","api","components","data","cards","searchResults","searchQuery","isSearchActive","created","fetchAllRecepti","window","addEventListener","updateSearchResults","beforeUnmount","removeEventListener","methods","response","get","error","console","event","detail","resetSearch"],"sources":["C:\\Users\\MATIN\\Desktop\\web aplikacije\\WA-frontend\\src\\views\\PocetnaStranica.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-3\">\r\n    <h1>Dobrodošli na našu slatku platformu!</h1>\r\n    <!-- Prikaz rezultata pretrage -->\r\n    <div v-if=\"isSearchActive\">\r\n      <h2>Rezultati pretrage za \"{{ searchQuery }}\"</h2>\r\n      <div v-if=\"searchResults.length > 0\">\r\n        <!-- Prikaz kartica za rezultate pretrage -->\r\n        <Card\r\n          v-for=\"card in searchResults\"\r\n          :key=\"card._id\"\r\n          :card=\"card\"\r\n          class=\"card-item\"\r\n        />\r\n      </div>\r\n      <div v-else>\r\n        <!-- Poruka ako nema rezultata -->\r\n        <h3>Nema rezultata za \"{{ searchQuery }}\"</h3>\r\n      </div>\r\n      <hr />\r\n      <button @click=\"resetSearch\" class=\"btn btn-secondary\">Prikaži sve recepte</button>\r\n    </div>\r\n\r\n    <!-- Prikaz svih recepata -->\r\n    <div v-else>\r\n      <h2>Svi recepti</h2>\r\n      <div>\r\n        <!-- Prikaz kartica za sve recepte -->\r\n        <Card\r\n          v-for=\"card in cards\"\r\n          :key=\"card._id\"\r\n          :card=\"card\"\r\n          class=\"card-item\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Card from \"../components/CardComponent.vue\";\r\nimport api from \"@/services/api\";\r\n\r\nexport default {\r\n  components: {\r\n    Card,\r\n  },\r\n  data() {\r\n    return {\r\n      cards: [], // Svi recepti iz baze\r\n      searchResults: [], // Rezultati pretrage\r\n      searchQuery: \"\", // Trenutni pojam pretrage\r\n      isSearchActive: false, // Stanje pretrage\r\n    };\r\n  },\r\n  async created() {\r\n    // Dohvati sve recepte prilikom učitavanja stranice\r\n    await this.fetchAllRecepti();\r\n\r\n    // Postavi listener za događaj pretrage\r\n    window.addEventListener(\"search-results\", this.updateSearchResults);\r\n  },\r\n  beforeUnmount() {\r\n    // Ukloni listener za pretragu prilikom uništenja komponente\r\n    window.removeEventListener(\"search-results\", this.updateSearchResults);\r\n  },\r\n  methods: {\r\n    async fetchAllRecepti() {\r\n      try {\r\n        const response = await api.get(\"/recepti\");\r\n        this.cards = response.data;\r\n      } catch (error) {\r\n        console.error(\"Greška pri dohvaćanju recepata:\", error);\r\n      }\r\n    },\r\n    updateSearchResults(event) {\r\n      // Ažuriraj rezultate pretrage\r\n      const { data, searchQuery } = event.detail;\r\n      this.searchResults = data;\r\n      this.searchQuery = searchQuery;\r\n      this.isSearchActive = true; // Aktiviraj stanje pretrage\r\n    },\r\n    resetSearch() {\r\n      // Resetiraj pretragu i vrati prikaz svih recepata\r\n      this.isSearchActive = false;\r\n      this.searchResults = [];\r\n      this.searchQuery = \"\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card-item {\r\n  width: 100%;\r\n}\r\n</style>\r\n"],"mappings":"AAwCA,OAAOA,IAAG,MAAO,iCAAiC;AAClD,OAAOC,GAAE,MAAO,gBAAgB;AAEhC,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MAAE;MACXC,aAAa,EAAE,EAAE;MAAE;MACnBC,WAAW,EAAE,EAAE;MAAE;MACjBC,cAAc,EAAE,KAAK,CAAE;IACzB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd;IACA,MAAM,IAAI,CAACC,eAAe,CAAC,CAAC;;IAE5B;IACAC,MAAM,CAACC,gBAAgB,CAAC,gBAAgB,EAAE,IAAI,CAACC,mBAAmB,CAAC;EACrE,CAAC;EACDC,aAAaA,CAAA,EAAG;IACd;IACAH,MAAM,CAACI,mBAAmB,CAAC,gBAAgB,EAAE,IAAI,CAACF,mBAAmB,CAAC;EACxE,CAAC;EACDG,OAAO,EAAE;IACP,MAAMN,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAMO,QAAO,GAAI,MAAMf,GAAG,CAACgB,GAAG,CAAC,UAAU,CAAC;QAC1C,IAAI,CAACb,KAAI,GAAIY,QAAQ,CAACb,IAAI;MAC5B,EAAE,OAAOe,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MACzD;IACF,CAAC;IACDN,mBAAmBA,CAACQ,KAAK,EAAE;MACzB;MACA,MAAM;QAAEjB,IAAI;QAAEG;MAAY,IAAIc,KAAK,CAACC,MAAM;MAC1C,IAAI,CAAChB,aAAY,GAAIF,IAAI;MACzB,IAAI,CAACG,WAAU,GAAIA,WAAW;MAC9B,IAAI,CAACC,cAAa,GAAI,IAAI,EAAE;IAC9B,CAAC;IACDe,WAAWA,CAAA,EAAG;MACZ;MACA,IAAI,CAACf,cAAa,GAAI,KAAK;MAC3B,IAAI,CAACF,aAAY,GAAI,EAAE;MACvB,IAAI,CAACC,WAAU,GAA