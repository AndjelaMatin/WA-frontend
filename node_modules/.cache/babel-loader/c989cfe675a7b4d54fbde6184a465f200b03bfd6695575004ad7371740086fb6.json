{"ast":null,"code":"import Card from \"../components/CardComponent.vue\";\nimport api from \"@/services/api\";\nexport default {\n  components: {\n    Card\n  },\n  data() {\n    return {\n      cards: [],\n      // Svi recepti\n      searchResults: [],\n      // Rezultati pretrage\n      searchQuery: \"\" // Trenutni pojam pretrage\n    };\n  },\n  async created() {\n    try {\n      // Dohvaćanje svih recepata\n      const response = await api.get(\"/recepti\");\n      this.cards = response.data;\n    } catch (error) {\n      console.error(\"Greška pri dohvaćanju podataka:\", error);\n    }\n\n    // Dodavanje listenera za pretragu\n    window.addEventListener(\"search-results\", this.updateSearchResults);\n  },\n  beforeUnmount() {\n    // Ukloni listener kada se komponenta uništi\n    window.removeEventListener(\"search-results\", this.updateSearchResults);\n  },\n  methods: {\n    updateSearchResults(event) {\n      const {\n        data,\n        searchQuery\n      } = event.detail;\n      console.log(\"Rezultati pretrage:\", data); // Provjera rezultata u konzoli\n      this.searchResults = data; // Dodijeli rezultate pretrage\n      this.searchQuery = searchQuery; // Postavi trenutni pojam pretrage\n    }\n  }\n};","map":{"version":3,"names":["Card","api","components","data","cards","searchResults","searchQuery","created","response","get","error","console","window","addEventListener","updateSearchResults","beforeUnmount","removeEventListener","methods","event","detail","log"],"sources":["C:\\Users\\MATIN\\Desktop\\web aplikacije\\WA-frontend\\src\\views\\PocetnaStranica.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-3\">\r\n    <h1>Dobrodošli na našu slatku platformu!</h1>\r\n    <!-- Rezultati pretrage -->\r\n    <div v-if=\"searchResults.length > 0\">\r\n      <h2>Rezultati pretrage za \"{{ searchQuery }}\"</h2>\r\n      <div>\r\n        <Card\r\n          v-for=\"card in searchResults\"\r\n          :key=\"card._id\"\r\n          :card