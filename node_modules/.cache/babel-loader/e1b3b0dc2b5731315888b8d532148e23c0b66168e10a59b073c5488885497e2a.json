{"ast":null,"code":"import Card from \"../components/CardComponent.vue\";\nimport api from '@/services/api';\nexport default {\n  components: {\n    Card\n  },\n  data() {\n    return {\n      cards: [],\n      searchResults: []\n    };\n  },\n  created() {\n    // Registracija listener-a samo jednom\n    if (!this.listenerRegistered) {\n      window.addEventListener(\"search-results\", this.updateSearchResults);\n      this.listenerRegistered = true; // Dodaj flag za sprječavanje višestrukih registracija\n    }\n\n    // Dohvati sve recepte\n    this.fetchAllRecepti();\n  },\n  beforeUnmount() {\n    // Ukloni listener kada se komponenta uništi\n    if (this.listenerRegistered) {\n      window.removeEventListener(\"search-results\", this.updateSearchResults);\n      this.listenerRegistered = false;\n    }\n  },\n  methods: {\n    async fetchAllRecepti() {\n      try {\n        const response = await api.get(\"/recepti\");\n        this.cards = response.data;\n      } catch (error) {\n        console.error(\"Greška pri dohvaćanju recepata:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["Card","api","components","data","cards","searchResults","created","listenerRegistered","window","addEventListener","updateSearchResults","fetchAllRecepti","beforeUnmount","removeEventListener","methods","response","get","error","console"],"sources":["C:\\Users\\MATIN\\Desktop\\web aplikacije\\WA-frontend\\src\\views\\PocetnaStranica.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-3\">\r\n    <h1>Dobrodošli na našu slatku platformu!</h1>\r\n    <div v-if=\"searchResults.length > 0\">\r\n      <h2>Rezultati pretrage</h2>\r\n      <div>\r\n        <Card\r\n          v-for=\"card in searchResults\"\r\n          :key=\"card.id\"\r\n          :card=\"card\"\r\n          class=\"card-item\"\r\n        />\r\n      </div>\r\n      <hr />\r\n    </div>\r\n    <div>\r\n      <h2>Svi recepti</h2>\r\n      <Card\r\n        v-for=\"card in cards\"\r\n        :key=\"card.id\"\r\n        :card=\"card\"\r\n        class=\"card-item\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Card from \"../components/CardComponent.vue\";\r\nimport api from '@/services/api';\r\n\r\nexport default {\r\n  components: {\r\n    Card,\r\n  },\r\n  data() {\r\n    return {\r\n      cards: [],\r\n      searchResults: [],\r\n    };\r\n  },\r\n  created() {\r\n  // Registracija listener-a samo jednom\r\n  if (!this.listenerRegistered) {\r\n    window.addEventListener(\"search-results\", this.updateSearchResults);\r\n    this.listenerRegistered = true; // Dodaj flag za sprječavanje višestrukih registracija\r\n  }\r\n\r\n  // Dohvati sve recepte\r\n  this.fetchAllRecepti();\r\n},\r\n\r\nbeforeUnmount() {\r\n  // Ukloni listener kada se komponenta uništi\r\n  if (this.listenerRegistered) {\r\n    window.removeEventListener(\"search-results\", this.updateSearchResults);\r\n    this.listenerRegistered = false;\r\n  }\r\n},\r\n\r\nmethods: {\r\n  async fetchAllRecepti() {\r\n    try {\r\n      const response = await api.get(\"/recepti\");\r\n      this.cards = response.data;\r\n    } catch (error) {\r\n      console.error(\"Greška pri dohvaćanju recepata:\", error);\r\n    }\r\n  },\r\n},\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card-item {\r\n  width: 100%;\r\n}\r\n</style>\r\n"],"mappings":"AA4BA,OAAOA,IAAG,MAAO,iCAAiC;AAClD,OAAOC,GAAE,MAAO,gBAAgB;AAEhC,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACV;IACA,IAAI,CAAC,IAAI,CAACC,kBAAkB,EAAE;MAC5BC,MAAM,CAACC,gBAAgB,CAAC,gBAAgB,EAAE,IAAI,CAACC,mBAAmB,CAAC;MACnE,IAAI,CAACH,kBAAiB,GAAI,IAAI,EAAE;IAClC;;IAEA;IACA,IAAI,CAACI,eAAe,CAAC,CAAC;EACxB,CAAC;EAEDC,aAAaA,CAAA,EAAG;IACd;IACA,IAAI,IAAI,CAACL,kBAAkB,EAAE;MAC3BC,MAAM,CAACK,mBAAmB,CAAC,gBAAgB,EAAE,IAAI,CAACH,mBAAmB,CAAC;MACtE,IAAI,CAACH,kBAAiB,GAAI,KAAK;IACjC;EACF,CAAC;EAEDO,OAAO,EAAE;IACP,MAAMH,eAAeA,CAAA,EAAG;MACt