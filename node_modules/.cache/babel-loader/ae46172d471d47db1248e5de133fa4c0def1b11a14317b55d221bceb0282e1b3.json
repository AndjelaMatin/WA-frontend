label>\r\n        <input id=\"naziv\" v-model=\"recept.naziv\" type=\"text\" required />\r\n  \r\n        <label for=\"sastojci\">Sastojci (odvojeni zarezima):</label>\r\n        <textarea id=\"sastojci\" v-model=\"recept.sastojci\"></textarea>\r\n  \r\n        <label for=\"upute\">Upute (odvojene novim redom):</label>\r\n        <textarea id=\"upute\" v-model=\"recept.upute\"></textarea>\r\n  \r\n        <label for=\"slika\">URL slike:</label>\r\n        <input id=\"slika\" v-model=\"recept.slika\" type=\"url\" />\r\n  \r\n        <label for=\"porcije\">Broj porcija:</label>\r\n        <input id=\"porcije\" v-model.number=\"recept.porcije\" type=\"number\" min=\"1\" />\r\n  \r\n        <button type=\"submit\">Spremi promjene</button>\r\n      </form>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import api from \"@/services/api\";\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        recept: {\r\n          naziv: \"\",\r\n          sastojci: \"\",\r\n          upute: \"\",\r\n          slika: \"\",\r\n          porcije: 1,\r\n        },\r\n      };\r\n    },\r\n    async created() {\r\n      try {\r\n        const { data } = await api.get(`/recepti/${this.$route.params.id}`);\r\n        this.recept = {\r\n          ...data,\r\n          sastojci: data.sastojci.join(\", \"),\r\n          upute: data.upute.join(\"\\n\"),\r\n        };\r\n      } catch (error) {\r\n        console.error(\"Greška pri dohvaćanju recepta:\", error);\r\n      }\r\n    },\r\n    methods: {\r\n      async azurirajRecept() {\r\n        try {\r\n          const token = localStorage.getItem(\"token\");\r\n          await api.put(\r\n            `/recepti/${this.$route.params.id}`,\r\n            {\r\n              ...this.recept,\r\n              sastojci: this.recept.sastojci.split(\",\").map((s) => s.trim()),\r\n              upute: this.recept.upute.split(\"\\n\").map((u) => u.trim()),\r\n            },\r\n            {\r\n              headers: { Authorization: `Bearer ${token}` },\r\n            }\r\n          );\r\n          alert(\"Recept uspješno ažuriran!\");\r\n          this.$router.push(\"/mojirecepti\");\r\n        } catch (error) {\r\n          console.error(\"Greška pri ažuriranju recepta:\", error);\r\n          alert(\"Došlo je do greške pri spremanju promjena.\");\r\n        }\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .edit-recipe-container {\r\n    max-width: 600px;\r\n    margin: 20px auto;\r\n    padding: 20px;\r\n    background-color: #fbf5e5;\r\n    border-radius: 10px;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  }\r\n  \r\n  h1 {\r\n    text-align: center;\r\n    color: #2a231f;\r\n  }\r\n  \r\n  form {\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n  \r\n  label {\r\n    font-weight: bold;\r\n    margin-bottom: 5px;\r\n    color: #c97d60;\r\n  }\r\n  \r\n  input,\r\n  textarea {\r\n    margin-bottom: 15px;\r\n    padding: 10px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 5px;\r\n  }\r\n  \r\n  button {\r\n    padding: 10px;\r\n    background-color: #2a231f;\r\n    color: #fff;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    font-size: 16px;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  button:hover {\r\n    background-color: #c97d60;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAyBE,OAAOA,GAAE,MAAO,gBAAgB;AAEhC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QACNC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAM;QAAEP;MAAK,IAAI,MAAMD,GAAG,CAACS,GAAG,CAAE,YAAW,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAG,EAAC,CAAC;MACnE,IAAI,CAACV,MAAK,GAAI;QACZ,GAAGD,IAAI;QACPG,QAAQ,EAAEH,IAAI,CAACG,QAAQ,CAACS,IAAI,CAAC,IAAI,CAAC;QAClCR,KAAK,EAAEJ,IAAI,CAACI,KAAK,CAACQ,IAAI,CAAC,IAAI;MAC7B,CAAC;IACH,EAAE,OAAOC,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,gCAAgC,EAAEA,KAAK,CAAC;IACxD;EACF,CAAC;EACDE,OAAO,EAAE;IACP,MAAMC