{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      novaNamirnica: \"\",\n      lista: []\n    };\n  },\n  methods: {\n    async dohvatiListu() {\n      try {\n        const response = await axios.get(\"http://localhost:5000/api/shoppingLista\");\n        this.lista = response.data;\n      } catch (error) {\n        console.error(\"Greška pri dohvaćanju liste:\", error);\n      }\n    },\n    async dodajNamirnicu() {\n      try {\n        if (!this.novaNamirnica.trim()) return;\n        const response = await axios.post(\"http://localhost:5000/api/shoppingLista\", {\n          name: this.novaNamirnica\n        });\n        this.lista.push(response.data);\n        this.novaNamirnica = \"\";\n      } catch (error) {\n        console.error(\"Greška pri dodavanju stavke:\", error);\n      }\n    },\n    async obrisiSveStavke() {\n      try {\n        await axios.delete(\"http://localhost:5000/api/shoppingLista\");\n        this.lista = [];\n      } catch (error) {\n        console.error(\"Greška pri brisanju stavki:\", error);\n      }\n    }\n  },\n  created() {\n    this.dohvatiListu();\n  }\n};","map":{"version":3,"names":["axios","data","novaNamirnica","lista","methods","dohvatiListu","response","get","error","console","dodajNamirnicu","trim","post","name","push","obrisiSveStavke","delete","created"],"sources":["C:\\Users\\MATIN\\Desktop\\web aplikacije\\WA-frontend\\src\\views\\ShoppingLista.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Lista namirnica za kupovinu:</h1>\r\n    <input v-model=\"novaNamirnica\" placeholder=\"Dodaj novu namirnicu...\" />\r\n    <button @click=\"dodajNamirnicu\">Dodaj namirnicu</button>\r\n    <button @click=\"obrisiSveStavke\">Obriši označene</button>\r\n    <ul>\r\n      <li v-for=\"(stavka, index) in lista\" :key=\"index\">\r\n        {{ stavka.name }}\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      novaNamirnica: \"\",\r\n      lista: [],\r\n    };\r\n  },\r\n  methods: {\r\n    async dohvatiListu() {\r\n      try {\r\n        const response = await axios.get(\"http://localhost:5000/api/shoppingLista\");\r\n        this.lista = response.data;\r\n      } catch (error) {\r\n        console.error(\"Greška pri dohvaćanju liste:\", error);\r\n      }\r\n    },\r\n    async dodajNamirnicu() {\r\n      try {\r\n        if (!this.novaNamirnica.trim()) return;\r\n        const response = await axios.post(\"http://localhost:5000/api/shoppingLista\", {\r\n          name: this.novaNamirnica,\r\n        });\r\n        this.lista.push(response.data);\r\n        this.novaNamirnica = \"\";\r\n      } catch (error) {\r\n        console.error(\"Greška pri dodavanju stavke:\", error);\r\n      }\r\n    },\r\n    async obrisiSveStavke() {\r\n      try {\r\n        await axios.delete(\"http://localhost:5000/api/shoppingLista\");\r\n        this.lista = [];\r\n      } catch (error) {\r\n        console.error(\"Greška pri brisanju stavki:\", error);\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.dohvatiListu();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  max-width: 700px;\r\n  margin: 20px auto;\r\n  padding: 20px;\r\n  background: #fbf5e5;\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.title {\r\n  text-align: center;\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n  margin-bottom: 20px;\r\n  color: #2a231f;\r\n}\r\n\r\n.button-container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  gap: 15px;\r\n}\r\n\r\ninput {\r\n  flex: 1;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n.button {\r\n  padding: 10px 20px;\r\n  background: #2a231f;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  font-size: 16px;\