ng\" class=\"profile-view\">\r\n      <h1 class=\"profile-name\">{{ user.name }}</h1>\r\n      <img :src=\"user.profilePicture\" alt=\"Profile Picture\" class=\"profile-picture\" />\r\n      <div class=\"profile-details\">\r\n        <p><strong>Email:</strong> {{ user.email }}</p>\r\n        <p><strong>Broj mobitela:</strong> {{ user.phoneNumber }}</p>\r\n      </div>\r\n      <button class=\"btn btn-primary\" @click=\"toggleEditing(true)\">Izmijeni</button>\r\n    </div>\r\n\r\n    <form v-else class=\"profile-edit-form\" @submit.prevent=\"updateUserData\">\r\n      <div class=\"form-group\">\r\n        <label for=\"name\">Ime:</label>\r\n        <input id=\"name\" v-model=\"editedUser.name\" type=\"text\" class=\"form-control\" />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"email\">Email:</label>\r\n        <input id=\"email\" v-model=\"editedUser.email\" type=\"email\" class=\"form-control\" />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"phone\">Broj mobitela:</label>\r\n        <input id=\"phone\" v-model=\"editedUser.phoneNumber\" type=\"tel\" class=\"form-control\" />\r\n      </div>\r\n\r\n      <div class=\"form-actions\">\r\n        <button class=\"btn btn-success\" type=\"submit\">Spremi promjene</button>\r\n        <button class=\"btn btn-secondary\" type=\"button\" @click=\"toggleEditing(false)\">Odustani</button>\r\n      </div>\r\n    </form>\r\n\r\n    <router-link to=\"/noviRecept\">\r\n      <button class=\"btn btn-outline-primary\" v-if=\"!isEditing\">Dodaj novi recept</button>\r\n    </router-link>\r\n  </div>\r\n  <div v-else class=\"loading-container\">\r\n    <p>Loading user data...</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport api from \"@/services/api\";\r\nimport { mapState, mapMutations } from \"vuex\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      editedUser: {},\r\n      isEditing: false,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState([\"user\"]),\r\n  },\r\n  created() {\r\n    if (!this.user) {\r\n      this.fetchUserData();\r\n    } else {\r\n      this.initializeEditedUser();\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapMutations([\"setUser\"]),\r\n\r\n    initializeEditedUser() {\r\n      this.editedUser = { ...this.user };\r\n    },\r\n\r\n    toggleEditing(isEditing) {\r\n      this.isEditing = isEditing;\r\n      if (!isEditing) {\r\n        this.initializeEditedUser();\r\n      }\r\n    },\r\n\r\n    async fetchUserData() {\r\n      try {\r\n        const token = localStorage.getItem(\"token\");\r\n        const response = await api.get(\"/auth/korisnici\", {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n        this.setUser(response.data);\r\n        this.initializeEditedUser();\r\n      } catch (error) {\r\n        console.error(\"Greška pri dohvaćanju korisnika:\", error);\r\n      }\r\n    },\r\n\r\n    async updateUserData() {\r\n      try {\r\n        const response = await api.put(`/auth/korisnici/${this.user.id}`, this.editedUser);\r\n        this.setUser(response.data);\r\n        this.toggleEditing(false);\r\n      } catch (error) {\r\n        console.error(\"Greška pri ažuriranju korisnika:\", error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.profile-container {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #ffffff;\r\n  border-radius: 12px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.profile-view {\r\n  text-align: center;\r\n}\r\n\r\n.profile-name {\r\n  font-size: 1.8em;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.profile-picture {\r\n  width: 120px;\r\n  height: 120px;\r\n  border-radius: 50%;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.profile-details {\r\n  text-align: left;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.profile-details p {\r\n  margin: 5px 0;\r\n  font-size: 1.1em;\r\n}\r\n\r\n.profile-edit-form {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 15px;\r\n}\r\n\r\n.form-group {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.form-group label {\r\n  margin-bottom: 5px;\r\n  font-weight: bold;\r\n}\r\n\r\n.form-control {\r\n  padding: 10px;\r\n  font-size: 1em;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n}\r\n\r\n.form-actions {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\n.btn {\r\n  padding: 10px 20px;\r\n  font-size: 1em;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.btn-primary {\r\n  background-color: #007bff;\r\n  color: #fff;\r\n}\r\n\r\n.btn-primary:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.btn-success {\r\n  background-color: #28a745;\r\n  color: #fff;\r\n}\r\n\r\n.btn-success:hover {\r\n  background-color: #218838;\r\n}\r\n\r\n.btn-secondary {\r\n  background-color: #6c757d;\r\n  color: #fff;\r\n}\r\n\r\n.btn-secondary:hover {\r\n  background-color: #5a6268;\r\n}\r\n\r\n.btn-outline-primary {\r\n  background-color: transparent;\r\n  color: #007bff;\r\n  border: 1px solid #007bff;\r\n}\r\n\r\n.btn-outline-primary:hover {\r\n  background-color: #007bff;\r\n  color: #fff;\r\n}\r\n\r\n.loading-container {\r\n  text-align: center;\r\n  font-size: 1.2em;\r\n  color: #666;\r\n}\r\n</style>\r\n"],"mappings":"AA4CA,OAAOA,GAAE,MAAO,gBAAgB;AAChC,SAASC,QAAQ,EAAEC,YAAW,QAAS,MAAM;AAE7C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,CAAC,CAAC;MACdC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGL,QAAQ,CAAC,CAAC,MAAM,CAAC;EACtB,CAAC;EACDM,OAAOA,CAAA,EAAG;IACR,IAAI,CAAC,IAAI,CAACC,IAAI,EAAE;MACd,IAAI,CAACC,aAAa,CAAC,CAAC;IACtB,OAAO;MACL,IAAI,CAACC,oBAAoB,CAAC,CAAC;IAC7B;EACF,CAAC;EACDC,OAAO,EAAE;IACP,GAAGT,YAAY,CAAC,CAAC,SAAS,CAAC,CAAC;IAE5BQ,oBAAoBA,CAAA,EAAG;MACrB,IAAI,CAACN,UAAS,GAAI;QAAE,GAAG,IAAI,CAACI;MAAK,CAAC;IACpC,CAAC;IAEDI,aAAaA,CAACP,SAAS,EAAE;MACvB,IAAI,CAACA,SAAQ,GAAIA,SAAS;MAC1B,IAAI,CAACA,SAAS,EAAE;QACd,IAAI,CAACK,oBAAoB,CAAC,CAAC;MAC7B;IACF,CAAC;IAED,MAAMD,aAAaA,CAAA,EAAG;MACpB,IAAI;QAC