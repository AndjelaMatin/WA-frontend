{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions } from 'vuex';\nimport api from \"@/services/api\";\nexport default {\n  data() {\n    return {\n      userName: \"\",\n      email: \"\",\n      password: \"\",\n      loginEmail: \"\",\n      loginPassword: \"\"\n    };\n  },\n  methods: {\n    ...mapActions([\"login\"]),\n    async signup() {\n      try {\n        // Poziv API-ja za registraciju\n        await api.post(\"/auth/signup\", {\n          name: this.userName,\n          email: this.email,\n          password: this.password\n        });\n\n        // Ako je uspješno, prikaži poruku\n        alert(\"Registracija uspješna! Prijavite se.\");\n        this.userName = \"\";\n        this.email = \"\";\n        this.password = \"\";\n      } catch (error) {\n        // Ako korisnik već postoji, prikazati odgovarajuću poruku\n        if (error.response && error.response.status === 409) {\n          alert(\"Korisnik s ovom email adresom već postoji.\");\n        } else {\n          alert(\"Registracija nije uspjela. Molimo pokušajte ponovo.\");\n        }\n        console.error(\"Signup error:\", error.response?.data || error);\n      }\n    },\n    async login() {\n      try {\n        const response = await api.post(\"/auth/login\", {\n          email: this.loginEmail,\n          password: this.loginPassword\n        });\n        if (response.data.token && response.data.user) {\n          console.log(\"Login API response: \", response);\n          this.$store.dispatch(\"login\", {\n            token: response.data.token,\n            user: response.data.user\n          });\n          console.log(\"Dispatched Vuex login action!\");\n          if (this.$route.path !== \"/\") {\n            this.$router.push(\"/\");\n          }\n        }\n      } catch (error) {\n        console.error(\"Login error:\", error.response?.data || error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapActions","api","data","userName","email","password","loginEmail","loginPassword","methods","signup","post","name","alert","error","response","status","console","login","token","user","log","$store","dispatch","$route","path","$router","push"],"sources":["C:\\Users\\MATIN\\Desktop\\web aplikacije\\WA-frontend\\src\\views\\LoginSignup.vue"],"sourcesContent":["<template>\r\n  <div class=\"main\">\r\n    <input type=\"checkbox\" id=\"chk\" aria-hidden=\"true\" />\r\n    <div class=\"signup\">\r\n      <form>\r\n        <label for=\"chk\" aria-hidden=\"true\">Sign up</label>\r\n        <input\r\n          v-model=\"userName\"\r\n          type=\"text\"\r\n          name=\"txt\"\r\n          placeholder=\"User name\"\r\n          required\r\n        />\r\n        <input\r\n          v-model=\"email\"\r\n          type=\"email\"\r\n          name=\"email\"\r\n          placeholder=\"Email\"\r\n          required\r\n        />\r\n        <input\r\n          v-model=\"password\"\r\n          type=\"password\"\r\n          name=\"pswd\"\r\n          placeholder=\"Lozinka\"\r\n          required\r\n        />\r\n        <button @click.prevent=\"signup\">Sign up</button>\r\n      </form>\r\n    </div>\r\n\r\n    <div class=\"login\">\r\n      <form>\r\n        <label for=\"chk\" aria-hidden=\"true\">Login</label>\r\n        <input\r\n          v-model=\"loginEmail\"\r\n          type=\"email\"\r\n          name=\"email\"\r\n          placeholder=\"Email\"\r\n          required\r\n        />\r\n        <input\r\n          v-model=\"loginPassword\"\r\n          type=\"password\"\r\n          name=\"pswd\"\r\n          placeholder=\"Lozinka\"\r\n          required\r\n        />\r\n        <button @click.prevent=\"login\">Login</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions } from 'vuex';\r\nimport api from \"@/services/api\";\r\n\r\nexport default {\r\ndata() {\r\n  return {\r\n    userName: \"\",\r\n    email: \"\",\r\n    password: \"\",\r\n    loginEmail: \"\",\r\n    loginPassword: \"\",\r\n  };\r\n},\r\nmethods: {\r\n  ...mapActions([\"login\"]),\r\n  async signup() {\r\n  try {\r\n    // Poziv API-ja za registraciju\r\n    await api.post(\"/auth/signup\", {\r\n      name: this.userName,\r\n      email: this.email,\r\n      password: this.password,\r\n    });\r\n\r\n    // Ako je uspješno, prikaži poruku\r\n    alert(\"Registracija uspješna! Prijavite se.\");\r\n    this.userName = \"\";\r\n    this.email = \"\";\r\n    this.password = \"\";\r\n  } catch (error) {\r\n    // Ako korisnik već postoji, prikazati odgovarajuću poruku\r\n    if (error.response && error.response.status === 409) {\r\n      alert(\"Korisnik s ovom email adresom već postoji.\");\r\n    } else {\r\n      alert(\"Registracija nije uspjela. Molimo pokušajte ponovo.\");\r\n    }\r\n    console.error(\"Signup error:\", error.response?.data || error);\r\n  }\r\n},\r\n  async login() {\r\n    try {\r\n      const response = await api.post(\"/auth/login\", {\r\n        email: this.loginEmail,\r\n        password: this.loginPassword,\r\n      });\r\n\r\n      if (response.data.token && response.data.user) {\r\n        console.log(\"Login API response: \", response);\r\n        this.$store.dispatch(\"login\", {\r\n          token: response.data.token,\r\n          user: response.data.user,\r\n        });\r\n        console.log(\"Dispatched Vuex login action!\");\r\n        if (this.$route.path !== \"/\") {\r\n          this.$router.push(\"/\");\r\n        }\r\n      }\r\n    } catch (error) {\r\n      console.error(\"Login error:\", error.response?.data || error);\r\n    }\r\n  },\r\n},\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.main {\r\nmargin: auto;\r\nwidth: 350px;\r\nheight: 500px;\r\nbackground: #fee6c1;\r\noverflow: hidden;\r\nbackground-color: #fee6c1;\r\nborder-radius: 10px;\r\nbox-shadow: 2px 10px 20px #2a231f;\r\n}\r\n#chk {\r\ndisplay: none;\r\n}\r\n.signup {\r\nposition: relative;\r\nwidth: 100%;\r\nheight: 100%;\r\n}\r\nlabel {\r\nco