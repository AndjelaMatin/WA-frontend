{"ast":null,"code":"import Card from \"../components/CardComponent.vue\";\nimport api from \"@/services/api\";\nexport default {\n  components: {\n    Card\n  },\n  data() {\n    return {\n      cards: [],\n      searchResults: [],\n      searchQuery: \"\" // Dodano za prikaz pretrage\n    };\n  },\n  async created() {\n    try {\n      // Dohvaćanje svih recepata\n      const response = await api.get(\"/recepti\");\n      this.cards = response.data;\n    } catch (error) {\n      console.error(\"Greška pri povlačenju podataka:\", error);\n    }\n\n    // Dodavanje listenera za pretragu\n    window.addEventListener(\"search-results\", this.updateSearchResults);\n  },\n  beforeUnmount() {\n    window.removeEventListener(\"search-results\", this.updateSearchResults);\n  },\n  methods: {\n    updateSearchResults(event) {\n      // Ažuriraj rezultate pretrage i sačuvaj query\n      this.searchResults = event.detail;\n      this.searchQuery = event.detail.length > 0 ? event.detail[0].searchQuery : \"\";\n    }\n  }\n};","map":{"version":3,"names":["Card","api","components","data","cards","searchResults","searchQuery","created","response","get","error","console","window","addEventListener","updateSearchResults","beforeUnmount","removeEventListener","methods","event","detail","length"],"sources":["C:\\Users\\MATIN\\Desktop\\web aplikacije\\WA-frontend\\src\\views\\PocetnaStranica.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-3\">\r\n    <h1>Dobrodošli na našu slatku platformu!</h1>\r\n    <!-- Rezultati pretrage -->\r\n    <div v-if=\"searchResults.length > 0\">\r\n      <h2>Rezultati pretrage</h2>\r\n      <div>\r\n        <Card\r\n          v-for=\"card in searchResults\"\r\n          :key=\"card._id\"\r\n          :card=\"card\"\r\n          class=\"card-item\"\r\n        />\r\n      </div>\r\n      <hr />\r\n    </div>\r\n    <!-- Poruka za praznu pretragu -->\r\n    <div v-else-if=\"searchQuery !== ''\">\r\n      <h3>Nema rezultata za pretragu \"{{