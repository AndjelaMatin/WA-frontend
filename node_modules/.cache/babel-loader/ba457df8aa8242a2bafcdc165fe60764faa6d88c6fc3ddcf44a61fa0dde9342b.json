for=\"recept in korisnik.omiljeniRecepti\" :key=\"recept._id\">\r\n            {{ recept.naziv }}\r\n          </li>\r\n        </ul>\r\n      </div>\r\n\r\n      <h2 class=\"footer-title\">Hvala što ste dio naše slatke zajednice!</h2>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      korisnik: {\r\n        ime: \"Vaše ime\", // Zamijeni s podacima korisnika\r\n        email: \"email@primjer.com\",\r\n        brojRecepata: 5,\r\n        omiljeniRecepti: [\r\n          { _id: \"1\", naziv: \"Torta od čokolade\" },\r\n          { _id: \"2\", naziv: \"Kolačići s maslacem\" },\r\n        ],\r\n        slika: \"\", // URL slike korisnika\r\n      },\r\n      defaultSlika: \"https://via.placeholder.com/150\", // Defaultna slika ako korisnik nema postavljenu sliku\r\n    };\r\n  },\r\n  async created() {\r\n    try {\r\n      // API poziv za dohvaćanje podataka korisnika\r\n      const response = await this.$api.get(\"/profil\");\r\n      this.korisnik = response.data;\r\n    } catch (error) {\r\n      console.error(\"Greška pri dohvaćanju profila korisnika:\", error);\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  max-width: 900px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  background-color: #fbf5e5;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.profil-header {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.profil-slika {\r\n  width: 150px;\r\n  height: 150px;\r\n  border-radius: 50%;\r\n  object-fit: cover;\r\n  border: 3px solid #2a231f;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.main-title {\r\n  font-size: 36px;\r\n  color: #2a231f;\r\n  text-align: center;\r\n  padding: 10px;\r\n  background-color: #ffe9c6;\r\n  border-radius: 8px;\r\n}\r\n\r\n.content-block {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.section-title {\r\n  font-size: 24px;\r\n  color: #444;\r\n  text-align: center;\r\n  margin-bottom: 10px;\r\n  bord