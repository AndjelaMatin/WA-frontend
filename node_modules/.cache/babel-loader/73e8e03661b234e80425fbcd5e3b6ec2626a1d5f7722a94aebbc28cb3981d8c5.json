{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport CardComponent from \"../components/CardComponent.vue\";\nimport api from \"@/services/api\";\nexport default {\n  components: {\n    CardComponent\n  },\n  data() {\n    return {\n      userRecipes: [] // Recepti korisnika\n    };\n  },\n  async created() {\n    try {\n      const token = localStorage.getItem(\"token\");\n      const response = await api.get(\"/mojirecepti\", {\n        headers: {\n          Authorization: `Bearer ${token}`\n        }\n      });\n      this.userRecipes = response.data;\n    } catch (error) {\n      console.error(\"Greška pri dohvaćanju recepata korisnika:\", error);\n    }\n  },\n  methods: {\n    editRecipe(id) {\n      // Navigacija na stranicu uređivanja\n      this.$router.push({\n        name: \"urediRecept\",\n        params: {\n          id\n        }\n      });\n    },\n    async deleteRecipe(id) {\n      // Brisanje recepta\n      if (!confirm(\"Jeste li sigurni da želite obrisati ovaj recept?\")) return;\n      try {\n        const token = localStorage.getItem(\"token\");\n        await api.delete(`/recepti/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.userRecipes = this.userRecipes.filter(recept => recept._id !== id);\n        alert(\"Recept je uspješno obrisan!\");\n      } catch (error) {\n        console.error(\"Greška pri brisanju recepta:\", error);\n        alert(\"Došlo je do greške. Pokušajte ponovno.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["CardComponent","api","components","data","userRecipes","created","token","localStorage","getItem","response","get","headers","Authorization","error","console","methods","editRecipe","id","$router","push","name","params","deleteRecipe","confirm","delete","filter","recept","_id","alert"],"sources":["C:\\Users\\MATIN\\Desktop\\web aplikacije\\WA-frontend\\src\\views\\MojiRecepti.vue"],"sourcesContent":["<template>\r\n    <div class=\"container mt-3\">\r\n      <h1 class=\"text-center mb-4\">Moji recepti</h1>\r\n      <div class=\"cards-container\">\r\n        <CardComponent\r\n          v-for=\"recept in userRecipes\"\r\n          :key=\"recept._id\"\r\n          :recept=\"recept\"\r\n          :showActions=\"true\" \r\n          @edit-recipe=\"editRecipe\"\r\n          @delete-recipe=\"deleteRecipe\"\r\n        />\r\n      </div>\r\n      <p v-if=\"userRecipes.length === 0\" class=\"text-center text-muted\">\r\n        Nemate nijedan recept. Dodajte novi recept kako bi se prikazao ovdje!\r\n      </p>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import CardComponent from \"../components/CardComponent.vue\";\r\n  import api from \"@/services/api\";\r\n  \r\n  export default {\r\n    components: {\r\n      CardComponent,\r\n    },\r\n    data() {\r\n      return {\r\n        userRecipes: [], // Recepti korisnika\r\n      };\r\n    },\r\n    async created() {\r\n      try {\r\n        const token = localStorage.getItem(\"token\");\r\n        const response = await api.get(\"/mojirecepti\", {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n        this.userRecipes = response.data;\r\n      } catch (error) {\r\n        console.error(\"Greška pri dohvaćanju recepata korisnika:\", error);\r\n      }\r\n    },\r\n    methods: {\r\n      editRecipe(id) {\r\n        // Navigacija na stranicu uređivanja\r\n        this.$router.push({ name: \"urediRecept\", params: { id } });\r\n      },\r\n      async deleteRecipe(id) {\r\n        // Brisanje recepta\r\n        if (!confirm(\"Jeste li sigurni da želite obrisati ovaj recept?\")) return;\r\n        try {\r\n          const token = localStorage.getItem(\"token\");\r\n          await api.delete(`/recepti/${id}`, {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          });\r\n          this.userRecipes = this.userRecipes.filter((recept) => recept._id !== id);\r\n          alert(\"Recept je uspješno obrisan!\");\r\n        } catch (error) {\r\n          console.error(\"Greška pri brisanju recepta:\", error);\r\n          alert(\"Došlo je do greške. Pokušajte ponovno.\");\r\n        }\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  \r\n  <style scoped>\r\n  .container {\r\n    max-width: 1200px;\r\n    margin: 0 auto;\r\n    padding: 20px;\r\n    background-color: #fbf5e5;\r\n    border-radius: 10px;\r\n    box-shadow: 0 6px 12px rgba(0, 0, 0, 0.1);\r\n  }\r\n  \r\n  .cards-container {\r\n    display: grid;\r\n    grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));\r\n    gap: 20px;\r\n    justify-items: center;\r\n  }\r\n  \r\n  .text-center {\r\n    font-size: 24px;\r\n    color: #2a231f;\r\n  }\r\n  \r\n  .text-muted {\r\n    color: #888;\r\n    font-size: 18px;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAoBE,OAAOA,aAAY,MAAO,iCAAiC;AAC3D,OAAOC,GAAE,MAAO,gBAAgB;AAEhC,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE,CAAE;IACnB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAC3C,MAAMC,QAAO,GAAI,MAAMR,GAAG,CAACS,GAAG,CAAC,cAAc,EAAE;QAC7CC,OAAO,EAAE;UAAEC,aAAa,EAAG,UAASN,KAAM;QAAE;MAC9C,CAAC,CAAC;MACF,IAAI,CAACF,WAAU,GAAIK,QAAQ,CAACN,IAAI;IAClC,EAAE,OAAOU,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,2CAA2C,EAAEA,KAAK,CAAC;IACnE;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,UAAUA,CAACC,EAAE,EAAE;MACb;MACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE,aAAa;QAAEC,MAAM,EAAE;UAAEJ;QAAG;MAAE,CAAC,CAAC;IAC5D,CAAC;IACD,MAAMK,YAAYA,CAACL,EAAE,EAAE;MACrB;MACA,IAAI,CAACM,OAAO,CAAC,kDAAkD,CAAC,EAAE;MAClE,IAAI;QACF,MAAMjB,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QAC3C,MAAMP,GAAG,CAACuB,MAAM,CAAE,YAAWP,EAAG,EAAC,EAAE;UACjCN,OAAO,EAAE;YAAEC,aAAa,EAAG,UAASN,KAAM;UAAE;QAC9C,CAAC,CAAC;QACF,IAAI,CAACF,WAAU,GAAI,IAAI,CAACA,WAAW,CAACqB,MAAM,CAAEC,MAAM,IAAKA,MAAM,CAACC,GAAE,KAAMV,EAAE,CAAC;QACzEW,KAAK,CAAC,6BAA6B,CAAC;MACtC,EAAE,OAAOf,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,8BA