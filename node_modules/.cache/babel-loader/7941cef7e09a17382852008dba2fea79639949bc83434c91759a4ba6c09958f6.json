r />\r\n    </div>\r\n    <div>\r\n      <h2>Svi recepti</h2>\r\n      <Card\r\n        v-for=\"card in cards\"\r\n        :key=\"card.id\"\r\n        :card=\"card\"\r\n        class=\"card-item\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Card from \"../components/CardComponent.vue\";\r\nimport api from '@/services/api';\r\n\r\nexport default {\r\n  components: {\r\n    Card,\r\n  },\r\n  data() {\r\n    return {\r\n      cards: [],\r\n      searchResults: [],\r\n    };\r\n  },\r\n  \r\n  async created() {\r\n    try {\r\n      const response = await api.get('/recepti');\r\n      const uniqueCards = response.data.filter((value, index, self) =>\r\n        index === self.findIndex((t) => t.id === value.id)\r\n      );\r\n      this.cards = uniqueCards;\r\n    } catch (error) {\r\n      console.error('Greška pri povlačenju podataka:', error);\r\n    }\r\n\r\n    window.addEventListener(\"search-results\", this.updateSearchResults);\r\n  },\r\n  beforeUnmount() {\r\n    window.removeEventListener(\"search-results\", this.updateSearchResults);\r\n  },\r\n  updateSearchResults(event) {\r\n  const { data, searchQuery } = event.detail;\r\n  this.searchResults = data;\r\n  this.searchQuery = searchQuery;\r\n  if (!data.length) {\r\n    console.log(\"Nema rezultata za pretragu:\", searchQuery);\r\n  }\r\n},\r\n  methods: {\r\n    updateSearchResults(event) {\r\n      this.searchResults = event.detail;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card-item {\r\n  width: 100%;\r\n}\r\n</style>\r\n"],"mappings":"AA4BA,OAAOA,IAAG,MAAO,iCAAiC;AAClD,OAAOC,GAAE,MAAO,gBAAgB;AAEhC,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EAED,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,QAAO,GAAI,MAAMN,GAAG,CAACO,GAAG,CAAC,UAAU,CAAC;MAC1C,MAAMC,WAAU,GAAIF,QAAQ,CAACJ,IAAI,CAACO,MAAM,CAAC,CAACC,KAAK,EAAEC,KAAK,EAAEC,IAAI,KAC1DD,KAAI,KAAMC,IAAI,CAACC,SAAS,CAAEC,CAAC,I