{"ast":null,"code":"import api from \"@/services/api\";\nimport { mapState } from \"vuex\";\nexport default {\n  computed: {\n    ...mapState([\"user\", \"isAuthenticated\"])\n  },\n  data() {\n    return {\n      passwords: {\n        currentPassword: \"\",\n        newPassword: \"\"\n      },\n      isEditing: false\n    };\n  },\n  methods: {\n    enableEditing() {\n      this.isEditing = true;\n    },\n    disableEditing() {\n      this.isEditing = false;\n      this.passwords = {\n        currentPassword: \"\",\n        newPassword: \"\"\n      }; // Reset form\n    },\n    async updatePassword() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await api.put(\"/auth/korisnici/password\", {\n          currentPassword: this.passwords.currentPassword,\n          newPassword: this.passwords.newPassword\n        }, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        alert(\"Lozinka uspješno ažurirana.\");\n        this.disableEditing();\n      } catch (error) {\n        console.error(\"Greška pri ažuriranju lozinke:\", error);\n        alert(\"Došlo je do greške pri ažuriranju lozinke.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["api","mapState","computed","data","passwords","currentPassword","newPassword","isEditing","methods","enableEditing","disableEditing","updatePassword","token","localStorage","getItem","put","headers","Authorization","alert","error","console"],"sources":["C:\\Users\\MATIN\\Desktop\\web aplikacije\\WA-frontend\\src\\views\\ProfilStranica.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\" v-if=\"user && user.name\">\r\n    <div v-if=\"!isEditing\">\r\n      <h1>{{ user.name }}</h1>\r\n      <h4>Email: {{ user.email }}</h4>\r\n      <button class=\"button\" @click=\"enableEditing\">Promijeni lozinku</button>\r\n    </div>\r\n\r\n    <form v-else @submit.prevent=\"updatePassword\">\r\n      <label>Trenutna lozinka:</label>\r\n      <input v-model=\"passwords.currentPassword\" type=\"password\" /><br />\r\n\r\n      <label>Nova lozinka:</label>\r\n      <input v-model=\"passwords.newPassword\" type=\"password\" /><br />\r\n\r\n      <button class=\"button\" type=\"submit\">Ažuriraj lozinku</button>\r\n      <button class=\"button\" @click=\"disableEditing\">Odustani</button>\r\n    </form>\r\n  </div>\r\n  <div v-else>\r\n    <p>Učitavanje korisničkih podataka...</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport api from \"@/services/api\";\r\nimport { mapState } from \"vuex\";\r\n\r\nexport default {\r\n  computed: {\r\n    ...mapState([\"user\", \"isAuthenticated\"]), \r\n  },\r\n  data() {\r\n    return {\r\n      passwords: { currentPassword: \"\", newPassword: \"\" },\r\n      isEditing: false,\r\n    };\r\n  },\r\n  methods: {\r\n    enableEditing() {\r\n      this.isEditing = true;\r\n    },\r\n    disableEditing() {\r\n      this.isEditing = false;\r\n      this.passwords = { currentPassword: \"\", newPassword: \"\" }; // Reset form\r\n    },\r\n    async updatePassword() {\r\n      try {\r\n        const token = localStorage.getItem(\"token\");\r\n        await api.put(\r\n          \"/auth/korisnici/password\",\r\n          {\r\n            currentPassword: this.passwords.currentPassword,\r\n            newPassword: this.passwords.newPassword,\r\n          },\r\n          {\r\n            headers: { Authorization: `Bearer ${token}` },\r\n          }\r\n        );\r\n        alert(\"Lozinka uspješno ažurirana.\");\r\n        this.disableEditing();\r\n      } catch (error) {\r\n        console.error(\"Greška pri ažuriranju lozinke:\", error);\r\n        alert(\"Došlo je do greške pri ažuriranju lozinke.\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAyBA,OAAOA,GAAE,MAAO,gBAAgB;AAChC,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,QAAQ,EAAE;IACR,GAAGD,QAAQ,CAAC,CAAC,MAAM,EAAE,iBAAiB,CAAC;EACzC,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;QAAEC,eAAe,EAAE,EAAE;QAA