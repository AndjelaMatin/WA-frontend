aceholder=\"Lozinka\"\r\n          required=\"\"\r\n        />\r\n        <button @click.prevent=\"login\">Login</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      userName: \"\",\r\n      email: \"\",\r\n      password: \"\",\r\n      loginEmail: \"\",\r\n      loginPassword: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    signup() {\r\n      console.log(\"Signup:\", this.userName, this.email, this.password);\r\n    },\r\n    login() {\r\n      console.log(\"Login:\", this.loginEmail, this.loginPassword);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Glavni kontejner */\r\n.main {\r\n  position: relative;\r\n  margin: auto;\r\n  width: 400px;\r\n  height: 600px;\r\n  background: linear-gradient(135deg, #ffe9c6, #fee6c1);\r\n  overflow: hidden;\r\n  border-radius: 15px;\r\n  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n/* Checkbox za prebacivanje */\r\n#chk {\r\n  display: none;\r\n}\r\n\r\n/* Sign up sekcija */\r\n.signup,\r\n.login {\r\n  position: absolute;\r\n  width: 100%;\r\n  height: 100%;\r\n  top: 0;\r\n  left: 0;\r\n  transition: transform 0.8s ease-in-out;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  flex-direction: column;\r\n}\r\n\r\n/* Label za prebacivanje */\r\n.form-label {\r\n  font-size: 24px;\r\n  color: #2a231f;\r\n  font-weight: bold;\r\n  margin-bottom: 20px;\r\n  transition: transform 0.5s ease-in-out;\r\n}\r\n\r\n/* Input polja */\r\ninput {\r\n  width: 80%;\r\n  height: 40px;\r\n  margin: 10px 0;\r\n  padding: 10px;\r\n  background: #fbf5e5;\r\n  color: #2a231f;\r\n  border: none;\r\n  outline: none;\r\n  border-radius: 5px;\r\n  box-shadow: 1px 2px 5px rgba(0, 0, 0, 0.1);\r\n  font-size: 16px;\r\n}\r\n\r\ninput:focus {\r\n  box-shadow: 1px 2px 10px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n/* Dugmad */\r\nbutton {\r\n  width: 80%;\r\n  height: 40px;\r\n  margin: 20px 0;\r\n  background: #2a231f;\r\n  color: #fff;\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background 0.3s ease, transform 0.2s ease;\r\n}\r\n\r\nbutton:hover {\r\n  background: #fbf5e5;\r\n  color: #2a231f;\r\n  transform: scale(1.05);\r\n}\r\n\r\n/* Login sekcija */\r\n.login {\r\n  background: #fbf5e5;\r\n  border-radius: 50% / 10%;\r\n  transform: translateY(-150%);\r\n  transition: transform 0.8s ease-in-out, border-radius 0.8s ease-in-out;\r\n}\r\n\r\n#chk:checked ~ .login {\r\n  transform: translateY(0);\r\n  border-radius: 15px;\r\n}\r\n\r\n#chk:checked ~ .signup {\r\n  transform: translateY(150%);\r\n}\r\n\r\n/* Animacije */\r\n.signup {\r\n  background: linear-gradient(135deg, #fee6c1, #ffe9c6);\r\n}\r\n</style>\r\n"],"mappings":"AAuDA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,EAAE;MACdC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACPC,OAAO,CAACC,GAAG,CAAC,SAAS,EAAE,IAAI,CAACR,QAAQ,EAAE,IAAI,CAACC,KAAK,EAAE,IAAI,CAACC,QAAQ,CAAC;IAClE,CAAC;IACDO,KAAKA,CAAA,EAAG;MACNF,OAAO,CAACC,GAAG,CAAC,QAAQ,EAAE,IAAI,CAACL,UAAU,EAAE,IAAI,CAACC,aAAa,CAAC;IAC5D;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}